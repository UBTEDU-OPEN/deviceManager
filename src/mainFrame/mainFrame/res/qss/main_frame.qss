QMainWindow#mainFrame{
    background: #1E232D;
    font-family: Microsoft YaHei;
}

QMenu { border-radius: 2px; background-color: #444A5A; color:#FFFFFF; padding: 0px 5px; }
QMenu::item { font-size: 14px; padding: 5px 37px; margin: 10px 0px; border-radius: 4px;}
QMenu::item:selected { background-color: #5B6479; }

QPushButton#file{background-color: #1E232D;color:white;border:0px;}
QPushButton#file:hover{background-color: #313745;}
QPushButton#file:pressed{background: #00A5FF;}
QPushButton#help{background-color: #1E232D;color:white;border:0px;}
QPushButton#help:hover{background-color: #313745;}
QPushButton#help:pressed{background: #00A5FF;}
QPushButton#min{border-image: url(:/res/images/ic_minimize.svg);}
QPushButton#min:hover{border-image: url(:/res/images/ic_minimize_mouseover.svg);}
QPushButton#min:pressed{border-image: url(:/res/images/ic_minimize_p.svg);}
QPushButton#max{border-image: url(:/res/images/ic_maximize.svg);}
QPushButton#max:hover{border-image: url(:/res/images/ic_maximize_mouseover.svg);}
QPushButton#max:pressed{border-image: url(:/res/images/ic_maximize_p.svg);}
QPushButton#close{border-image: url(:/res/images/ic_close.svg);}
QPushButton#close:hover{border-image: url(:/res/images/ic_close_mouseover.svg);}
QPushButton#close:pressed{border-image: url(:/res/images/ic_close_p.svg);}

