renamewidget QLabel,QPushButton,QLineEdit
{
    color: white;

}

renamewidget #centralwidget
{
    background: #444A5A;
    border-radius: 4px;
}

renamewidget #titlebar
{
    height: 36px;
    background: #505769;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;

}

renamewidget QPushButton#pbclose{border-image: url(:/res/images/ic_close.svg);}
