renamewidget QPushButton#pbclose:pressed{border-image: url(:/res/images/ic_close_p.svg);}

renamewidget QPushButton#pbok
{
    background: #00A5FF;
    border-radius: 3px;
}

renamewidget QPushButton#pbcancel
{
    background: #5B6479;
    border-radius: 3px;
}

renamewidget QLineEdit#lineEdit
{
    background-color:#313747;
    border-radius:3px;
}

